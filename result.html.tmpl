<html>
<head>
  <meta charset="utf-8">
  <title>Performance measurement results</title>
</head>
<body>
  {% set keys = result.keys() %}
  <table border="1">
    <tr>
      <th></th>
      {% for key in keys %}
      <th>{{key}}</th>
      {% endfor %}
    </tr>
    <tr>
      <td>Time spent</td>
      {% for key in keys %}
      <td>{{result[key]['time'] | round(3)}} s</td>
      {% endfor %}
    </tr>
    {% for name, description in [
      ('memInitial', 'Initial memory usage'),
      ('memFinal', 'Final memory usage'),
      ('memEmscriptenUsed', 'Of that, used by Emscripten'),
    ] %}
    <tr>
      <td>{{description}}</td>
      {% for key in keys %}
      {% if name in result[key] %}
      <td>{{result[key][name] | filesizeformat(True)}}</td>
      {% else %}
      <td></td>
      {% endif %}
      {% endfor %}
    </tr>
    {% endfor %}
  </table>
</body>
</html>
